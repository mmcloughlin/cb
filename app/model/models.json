[
  {
    "name": "Commit",
    "doc": "Commit in the Go repository.",
    "collection": "commits",
    "id": "SHA",
    "fields": [
      { "name": "SHA" },
      { "name": "Tree" },
      { "name": "Parents", "type": "[]string" },
      { "name": "AuthorName" },
      { "name": "AuthorEmail" },
      { "name": "AuthorTime", "type": "time.Time" },
      { "name": "CommitterName" },
      { "name": "CommitterEmail" },
      { "name": "CommitTime", "type": "time.Time" },
      { "name": "Message" }
    ]
  },
  {
    "name": "Module",
    "doc": "Module is a Go module at a specific version.",
    "collection": "modules",
    "id": "UUID",
    "fields": [{ "name": "UUID" }, { "name": "Path" }, { "name": "Version" }]
  },
  {
    "name": "Package",
    "doc": "Package is a Go package within a versioned module.",
    "collection": "packages",
    "id": "UUID",
    "fields": [
      { "name": "UUID" },
      { "name": "ModuleUUID" },
      { "name": "RelativePath" }
    ]
  },
  {
    "name": "Benchmark",
    "doc": "Benchmark is a single benchmark measurement within a package.",
    "collection": "benchmarks",
    "id": "UUID",
    "fields": [
      { "name": "UUID" },
      { "name": "PackageUUID" },
      { "name": "FullName" },
      { "name": "Name" },
      { "name": "Unit" },
      { "name": "Parameters", "type": "map[string]string" }
    ]
  },
  {
    "name": "DataFile",
    "doc": "DataFile is an output file from a benchmark run.",
    "collection": "datafiles",
    "id": "UUID",
    "fields": [
      { "name": "UUID" },
      { "name": "Name" },
      { "name": "SHA256", "type": "[]byte", "key": "sha256" }
    ]
  },
  {
    "name": "Result",
    "doc": "Result from running a benchmark with a specific Go version.",
    "collection": "results",
    "id": "UUID",
    "fields": [
      { "name": "UUID" },
      { "name": "DataFileUUID" },
      { "name": "Line", "type": "int" },
      { "name": "BenchmarkUUID" },
      { "name": "CommitSHA" },
      { "name": "EnvironmentUUID" },
      { "name": "MetadataUUID" },
      { "name": "Iterations", "type": "uint64" },
      { "name": "Value", "type": "float64" }
    ]
  },
  {
    "name": "Properties",
    "doc": "Properties is a collection of key-value pairs.",
    "collection": "properties",
    "id": "UUID",
    "fields": [
      { "name": "UUID" },
      { "name": "Fields", "type": "map[string]string" }
    ]
  }
]
