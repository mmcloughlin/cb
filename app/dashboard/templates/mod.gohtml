{{ define "title" }}{{ .Module.Path }}{{ end }}

{{ define "content" }}
<h1>Module {{ .Module.Path }}</h1>
<div class="meta">
  <dl>
    <dt>Version</dt> <dd>{{ .Module.Version }}</dd>
  </dl>
</div>

<table>
  <tr>
    <th>Package</th>
  </tr>
  {{ range .Packages }}
  <tr>
    <td>{{ template "pkg" . }}</td>
  </tr>
  {{ end }}
</table>
{{ end }}
